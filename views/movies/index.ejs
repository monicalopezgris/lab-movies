<div>
  <h2>Celebrities list</h2>
  <% film.forEach((movie) => {%>
    <a href="movies/<%- movie._id%>">
      <%- movie.title%>
    </a> 
    <form action="movies/<%- movie._id%>/delete" method="post">
      <button type="submit">Delete</button>
    </form>
    </br>
  <%})%>
  <a href="movies/new">
    <button>Create new</button>
  </a>
</div>
