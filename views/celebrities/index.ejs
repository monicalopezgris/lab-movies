<div>
  <h2>Celebrities list</h2>
  <% celeb.forEach((celebrity) => {%>
    <div class='list'>
    <a href="celebrities/<%- celebrity._id%>">
      <%- celebrity.name%>
    </a> 
    <form action="celebrities/<%- celebrity._id%>/delete" method="post">
      <button type="submit">Delete</button>
    </form>
  </div>
  <%})%>
  <a href="celebrities/new">
    <button>Create new</button>
  </a>
</div>
